# - Try to find libquentier
# Once done this will define
#  Libquentier::libqt5quentier or Libquentier::libqt6quentier - imported target for libquentier library
#  LIBQUENTIER_HAS_NOTE_EDITOR - Boolean set to true if libquentier was built with NoteEditor

@LIB_QUENTIER_HAS_NOTE_EDITOR_OPTION@

unset(LIBQUENTIER_FIND_DEPS_ARGS)
if(${CMAKE_FIND_PACKAGE_NAME}_FIND_QUIETLY)
  list(APPEND LIBQUENTIER_FIND_DEPS_ARGS QUIET)
endif()
if(${CMAKE_FIND_PACKAGE_NAME}_FIND_REQUIRED)
  list(APPEND LIBQUENTIER_FIND_DEPS_ARGS REQUIRED)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/LibquentierFindDependencies.cmake")
include("${CMAKE_CURRENT_LIST_DIR}/Libquentier-qt@LIBQUENTIER_QT_VERSION@LibraryDepends.cmake")
